
{
  "expo": {
    "name": "SamSafeNet",
    "slug": "SamSafeNet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "permissions": [
            "ACCESS_FINE_LOCATION",
            "ACCESS_COARSE_LOCATION"
          ]
        }
      ],
      [
        "expo-bluetooth",
        {
          "permissions": [
            "BLUETOOTH",
            "BLUETOOTH_ADMIN"
          ]
        }
      ]
    ]
  }
}